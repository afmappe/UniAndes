package edu.uniandes.ecos.PSP2;

import java.util.Arrays;

public class Temp {
	private double xSum;
	private double ySum;

	public void LinearRegression(double[] xVals, double[] yVals) {
		if (xVals != null & yVals != null & xVals.length == yVals.length) {
			xSum = Arrays.stream(xVals).reduce(0, (x, y) -> x + y);
			ySum = Arrays.stream(yVals).reduce(0, (x, y) -> x + y);

			int a = 0;
		}
	}
}
